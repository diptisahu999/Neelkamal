<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="stpl_indiamart_connector_cron" model="ir.cron">
            <field name="name">STPL IndiaMart Connector Cron</field>
            <field name="active" eval="True"/>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="user_id" ref="base.user_admin"/>
            <field eval="False" name="doall"/>
            <field name="model_id" ref="stpl_indiamart_connector.model_crm_lead"/>
            <field name="code">model.call_stpl_indiamart_connector()</field>
            <field name="state">code</field>
            <field name="doall" eval="True"/>
        </record>

    </data>
</odoo>

